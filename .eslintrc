{
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "globals": {
        "React": false,
        "ReactDOM": false
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "rules": {
        "no-cond-assign": 0,
        "no-console": 1,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 1,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 1,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 1,
        "no-obj-calls": 2,
        "no-prototype-builtins": 0,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 2,
        "no-template-curly-in-string": 1,
        "no-unexpected-multiline": 1,
        "no-unreachable": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-negation": 2,
        "use-isnan": 1,
        "valid-jsdoc": 1,
        "valid-typeof": 2,
        "accessor-pairs": 1,
        "array-callback-return": 2,
        "block-scoped-var": 2,
        "class-methods-use-this": 1,
        "consistent-return": 0,
        "curly": [
            1,
            "all"
        ],
        "default-case": 1,
        "dot-location": 1,
        "dot-notation": 1,
        "eqeqeq": 2,
        "guard-for-in": 0,
        "no-alert": 1,
        "no-caller": 1,
        "no-case-declarations": 1,
        "no-div-regex": 2,
        "no-else-return": 1,
        "no-empty-function": 0,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 1,
        "no-extend-native": 2,
        "no-extra-bind": 1,
        "no-extra-label": 1,
        "no-fallthrough": 0,
        "no-floating-decimal": 2,
        "no-global-assign": 2,
        "no-implicit-coercion": 1,
        "no-implicit-globals": 0,
        "no-implied-eval": 1,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 0,
        "no-lone-blocks": 1,
        "no-loop-func": 0,
        "no-magic-numbers": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-new-func": 1,
        "no-new-wrappers": 1,
        "no-new": 0,
        "no-octal-escape": 2,
        "no-octal": 1,
        "no-param-reassign": 0,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-restricted-properties": 2,
        "no-return-assign": 0,
        "no-script-url": 1,
        "no-self-assign": 1,
        "no-self-compare": 1,
        "no-sequences": 2,
        "no-throw-literal": 1,
        "no-unmodified-loop-condition": 1,
        "no-unused-expressions": 1,
        "no-unused-labels": 1,
        "no-useless-call": 1,
        "no-useless-concat": 1,
        "no-useless-escape": 1,
        "no-void": 0,
        "no-warning-comments": 1,
        "no-with": 2,
        "radix": 1,
        "vars-on-top": 1,
        "wrap-iife": 0,
        "yoda": 1,
        "strict": 0,
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    }
}
